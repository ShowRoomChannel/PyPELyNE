	GROUP P_IMP_DISPLAY
	{
		LONG P_IMP_DISPLAY_MODE
		{
			CYCLE
			{
				P_IMP_TPDEFAULT;
				P_IMP_PARTICLES;
				P_IMP_CIRCLES;
				P_IMP_BOX;
			}
		}
		LONG P_IMP_CIRCLES_DIR
		{
			CYCLE
			{
				P_IMP_CAMERA;
				P_IMP_XY;
				P_IMP_YZ;
				P_IMP_XZ;
			}
		}
		REAL P_IMP_RADIUS {MIN 0.0; STEP 0.01;}
		LONG P_IMP_COLOR_MODE
		{
			CYCLE
			{
				P_IMP_COLOR_NONE;
				P_IMP_COLOR_VELOCITY;
				P_IMP_COLOR_NORMAL;
				P_IMP_COLOR_FORCE;
				P_IMP_COLOR_TEXTUREU;
				P_IMP_COLOR_TEXTUREV;
				P_IMP_COLOR_NEIGHBOR;
				P_IMP_COLOR_AGE;
				P_IMP_COLOR_ISOLATION;
				P_IMP_COLOR_VISCOSITY;
				P_IMP_COLOR_DENSITY;
				P_IMP_COLOR_PRESSURE;
				P_IMP_COLOR_MASS;
				P_IMP_COLOR_TEMPERATURE;
				P_IMP_COLOR_VORTICITY;
			}
		}
		GRADIENT P_IMP_DISP_COLOR {COLOR;}
		BOOL P_IMP_DISPLAY_AUTO_RANGE {}
		REAL P_IMP_DISPLAY_MIN {}
		REAL P_IMP_DISPLAY_MAX {}
		REAL P_IMP_DISPLAY_CURRENT_MIN {}
		REAL P_IMP_DISPLAY_CURRENT_MAX {}
	}

	GROUP P_IMP_GRP_RENDER
	{
		LONG P_IMP_RENDER_TYPE
		{
			CYCLE
			{
				P_IMP_RENDER_TYPE_POINT;
				P_IMP_RENDER_TYPE_SPHERE;
				P_IMP_RENDER_TYPE_SPRITE;
				P_IMP_RENDER_TYPE_OBJECT;
			}
		}
		REAL P_IMP_RENDER_RADIUS {MIN 0.0; UNIT METER;}
		LINK P_IMP_RENDER_OBJECT {ACCEPT {Obase;}}
      BUTTON P_IMP_ADD_PARTICLE_SHADER {}
	}
}
