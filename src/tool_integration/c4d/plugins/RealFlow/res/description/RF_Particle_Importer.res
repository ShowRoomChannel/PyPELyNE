CONTAINER RF_PARTICLE_IMPORTER
{
	NAME RFParticleImporter;
	INCLUDE Obase;

	GROUP P_IMP_SETUP
	{
		DEFAULT 1;
		
		FILENAME P_IMP_FILE_PATH {}
		STATICTEXT P_IMP_NAME {ANIM OFF;}
		LONG P_IMP_PADDING {MINSLIDER 0; MAXSLIDER 10;}
		LONG P_IMP_CACHE_MODE
		{
			CYCLE
			{
				P_IMP_CURRENT_FRAME;
				P_IMP_CORE_FRAME;
				P_IMP_ALL_FRAME;
			}
		}
		LONG P_IMP_CACHE_CORE {MIN 0;}
		LONG P_IMP_START_FRAME {MIN 0;}
		LONG P_IMP_END_FRAME {MIN 0;}
		LONG P_IMP_OFFSET {}
		REAL P_IMP_LOD {MIN 0.0; MAX 100.0; CUSTOMGUI REALSLIDER;}
		REAL P_IMP_SCALE {MIN 0.0;}
      REAL P_IMP_BLUR_LENGTH {MIN 0.0;}
		BASETIME P_IMP_LIFE {}
		BOOL P_IMP_KEEP_ORDER {}
		BOOL P_IMP_LOCK_CURRENT {}
		BOOL P_IMP_LOCK_LAST {}
		BOOL P_IMP_INVERT_SEQUENCE {}
      SEPARATOR {LINE;}
      BOOL P_IMP_SHARE_ASSETS {}
	}

